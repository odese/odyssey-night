	"semanticHighlighting": true,
	"semanticTokenColors": {
		"enumMember": {
			"foreground": "#56b6c2"
		},
		"variable.constant": {
			"foreground": "#d19a66"
		},
		"variable.defaultLibrary": {
			"foreground": "#e5c07b"
		},
		"variable:dart": {
			"foreground": "#d19a66"
		},
		"property:dart": {
			"foreground": "#d19a66"
		},
		"annotation:dart": {
			"foreground": "#d19a66"
		},
		"parameter.label:dart": {
			"foreground": "#abb2bf"
		},
		"macro": {
			"foreground": "#d19a66"
		}
	},
	"tokenColors": [
        {
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#40a13d",
                "fontStyle": "italic"
			}
		},
		// YAML
		{
			"name": "YAML Entity Name",
			"scope": "entity.name.tag.yaml",
			"settings": {
				"foreground": "#24dd6e"
			}
		},
		{
			"name": "YAML String",
			"scope": [
				"string.unquoted.plain.out.yaml", //
				"string.quoted.single.yaml",
				"string.quoted.double.yaml"
			],
			"settings": {
				"foreground": "#c84b61"
			}
		},
		{
			"name": "YAML Integer",
			"scope": [
                "constant.numeric.integer.yaml", //
                "constant.numeric.float.yaml",
            ],
			"settings": {
				"foreground": "#00aeff"
			}
		},
		{
			"name": "YAML Boolean",
			"scope": "constant.language.boolean.yaml",
			"settings": {
				"foreground": "#ffe302"
			}
		},
		// JSON
		{
			"name": "JSON Property Name",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#72f1b8"
			}
		},
		{
			"name": "JSON String",
			"scope": [
				"string.quoted.double.json", //
				"string.quoted.single.json",
				"string.quoted.triple.json"
			],
			"settings": {
				"foreground": "#ff8b39"
			}
		},
		{
			"name": "JSON Numeric",
			"scope": "constant.numeric.json",
			"settings": {
				"foreground": "#00aeff"
			}
		},
        {
			"name": "JSON boolean",
			"scope": "constant.language.json",
			"settings": {
				"foreground": "#b657ff"
			}
		},
        // Go
		{
			"name": "Raw String (JSON Tag)",
			"scope": "string.quoted.raw",
			"settings": {
				"foreground": "#c84b61"
			}
		},
        // Storage Type Colors
        {
			"name": "Storage Types",
			"scope": "storage.type",
			"settings": {
				"foreground": "#3bbea4",
                "fontStyle": "italic"
			}
		},
        {
			"name": "Error Type",
			"scope": "storage.type.error",
			"settings": {
				"foreground": "#d60076",
                "fontStyle": "italic"
			}
		},
        // Keyword Colors
        {
			"name": "Interface & Struct Keyword",
			"scope": [
                "keyword.struct", //
                "keyword.interface"
            ],
			"settings": {
				"foreground": "#6b9aff",
                "fontStyle": "italic"
            }
		},
        {
            "name": "Map Keyword",
            "scope": "keyword.map",
            "settings": {
                "foreground": "#d468f5",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Type Keyword",
            "scope": "keyword.type",
            "settings": {
                "foreground": "#ad33ff"
            }
        },
        {
            "name": "Constant Keyword",
            "scope": "keyword.const",
            "settings": {
                "foreground": "#ad33ff"
            }
        },
        {
            "name": "Var Keyword",
            "scope": "keyword.var",
            "settings": {
                "foreground": "#3d71ff"
            }
        },
        {
            "name": "Package & Import Keywords",
            "scope": [
                "keyword.package",//
                "keyword.import"
            ],
            "settings": {
                "foreground": "#3d71ff"
            }
        },
        {
            "name": "Function Keyword",
            "scope": "keyword.function",
            "settings": {
                "foreground": "#ad33ff",
            }
        },
        {
            "name": "Control Keywords",
            "scope": "keyword.control",
            "settings": {
                "foreground": "#ff0096"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ff0096"
            }
        },
        // Support
        {
            "name": "Function Name (Call)",
            "scope": "support.function",
            "settings": {
                "foreground": "#0e9fe3",
            }
        },
        // Entity & Variable Colors
        {
            "name": "Function Name (Declaration)",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#ffb401",
            }
        },
        {
            "name": "Type Name",
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#ffb401",
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable.other.assignment",//
                "variable.other.declaration"
            ],
            "settings": {
                "foreground": "#2ee2fa"
            }
        },
        {
            "name": "Import Name",
            "scope": "entity.name.import",
            "settings": {
                "foreground": "#b87e70"
            }
        },
        {
            "name": "Import Alias",
            "scope": "entity.alias.import",
            "settings": {
                "foreground": "#95d0f0"
            }
        },
        // Values
        {
            "name": "Nil & Boolean Values",
            "scope": "constant.language",
            "settings": {
                "foreground": "#f35de6"
            }
        },
        {
            "name": "Numeric Values",
            "scope": "source.go constant.numeric",
            "settings": {
                "foreground": "#fede5d"
            }
        },
        {
            "name": "String Value",
            "scope": "string.quoted",
            "settings": {
                "foreground": "#ff8b39"
            }
        },
	]
}
